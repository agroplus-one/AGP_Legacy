	<queryString>
		<![CDATA[SELECT ASE.NOMBRE || ' ' || ASE.APELLIDO1 || ' ' || ASE.APELLIDO2 AS NOMBRE,
       ASE.RAZONSOCIAL AS ASEG_RAZON,
       ASE.TIPOIDENTIFICACION,
       TOM.RAZONSOCIAL,
       LIN.NOMLINEA,
       L.NOMLOCALIDAD,
       substr(PP.CCCBANCO,0,4) as c1,
       substr(PP.CCCBANCO,5,4) as c2,
       substr(PP.CCCBANCO,9,4) as c3,
       substr(PP.CCCBANCO,13,4) as c4,
       substr(PP.CCCBANCO,17,4) as c5,
       PP.IBAN,
      to_char(NVL(po.fechaenvio,sysdate),'DD "de" MONTH "de" YYYY', 'NLS_DATE_LANGUAGE = spanish') as FENVIO

  FROM o02agpe0.TB_POLIZAS      PO,
       o02agpe0.TB_ASEGURADOS   ASE,
       o02agpe0.TB_COLECTIVOS   CO,
       o02agpe0.TB_TOMADORES    TOM,
       o02agpe0.TB_LINEAS       LIN,
       o02agpe0.TB_PAGOS_POLIZA PP,
       o02agpe0.TB_LOCALIDADES  L,
       o02agpe0.TB_COMUNICACIONES c

 WHERE PO.IDASEGURADO = ASE.ID
   AND PP.IDPOLIZA(+) = PO.IDPOLIZA
   AND PO.IDCOLECTIVO = CO.ID
   AND CO.CIFTOMADOR = TOM.CIFTOMADOR
   AND TOM.CODENTIDAD = ASE.CODENTIDAD
   AND PO.LINEASEGUROID = LIN.LINEASEGUROID
   AND TOM.CODPROVINCIA = L.CODPROVINCIA
   AND TOM.SUBLOCALIDAD = L.SUBLOCALIDAD
   AND TOM.CODLOCALIDAD = L.CODLOCALIDAD
   AND PO.IDENVIO = C.IDENVIO (+)
   AND PO.IDPOLIZA = $P{ID}]]>
