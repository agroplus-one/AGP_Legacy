	<parameter name="CODCONCEPTO" class="java.lang.String"/>
	<queryString>
		<![CDATA[select LPAD (tc.codtipocapital, 3, '0') || ' - ' || tc.destipocapital NOMBRE, tb_aux.*
from o02agpe0.tb_sc_c_tipo_capital tc, (
SELECT COUNT(distinct (PA.IDPARCELA)) AS NUM_PARCELAS,
       ca.codtipocapital AS CODTIPOCAPITAL,
       SUM(NVL(REL.Produccionmodif, REL.PRODUCCION)) AS PRODUCCION,
       SUM(CA.SUPERFICIE) AS SUPERFICIE,
       SUM(NVL(REL.Preciomodif * REL.Produccionmodif, REL.PRECIO * REL.PRODUCCION)) AS PRECIO_PRODUCCION,
       NVL(SUM(NVL(REL.Preciomodif, REL.PRECIO) * to_number(CA.SUPERFICIE)),  null) AS PRECIO_SUPERFICIE
  FROM o02agpe0.TB_PARCELAS             PA,
       o02agpe0.TB_CAPITALES_ASEGURADOS CA,
       o02agpe0.TB_SC_C_TIPO_CAPITAL    TC,
       o02agpe0.TB_CAP_ASEG_REL_MODULO  REL
 WHERE CA.IDPARCELA = PA.IDPARCELA
   AND CA.CODTIPOCAPITAL = TC.CODTIPOCAPITAL
   AND CA.IDCAPITALASEGURADO = REL.IDCAPITALASEGURADO
   AND TC.CODCONCEPTO = $P{CODCONCEPTO}
   AND PA.HOJA = $P{HOJA}
   AND PA.IDPOLIZA = $P{IDPOLIZA}
   group by ca.codtipocapital
   ) tb_aux
   where tc.codtipocapital = tb_aux.CODTIPOCAPITAL
   order by tc.codtipocapital asc]]>
	</queryString>
