	<queryString>
		<![CDATA[select distinct a.NIFCIF,
	(A.NOMBRE ||' '|| a.apellido1 ||' '|| a.apellido2) as asegurado_nombre,
        (v.abreviatura||' '|| a.direccion ||' '|| a.numvia) as asegurado_domicilio,
        a.piso as asegurado_piso,
	a.bloque as asegurado_bloque,
	a.escalera as asegurado_escalera,
        pr.nomprovincia as asegurado_provincia,
	l.nomlocalidad as asegurado_localidad,
	a.telefono as asegurado_telefono,
	a.movil as asegurado_movil,
	a.email as asegurado_email,
	a.codpostal as asegurado_cp,
	a.tipoidentificacion as tipo,
        a.razonsocial as asegurado_razon
             from o02agpe0.tb_polizas p, o02agpe0.tb_asegurados a, o02agpe0.tb_vias v, o02agpe0.tb_localidades l, o02agpe0.tb_provincias pr
        where p.idasegurado = a.id
        and v.clave = a.clavevia
        and l.codprovincia = a.codprovincia
        and l.codlocalidad = a.codlocalidad
        and l.sublocalidad = a.sublocalidad
        and pr.codprovincia = a.codprovincia
        and p.idpoliza = $P{ID}]]>
	</queryString>
