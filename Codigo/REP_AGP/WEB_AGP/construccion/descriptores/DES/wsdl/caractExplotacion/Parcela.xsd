<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.agroseguro.es/Contratacion/Parcela"
	xmlns="http://www.agroseguro.es/Contratacion/Parcela"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:ti="http://www.agroseguro.es/Tipos" 
	xmlns:dv="http://www.agroseguro.es/Contratacion/DatosVariables"
>
	<xs:import
		namespace="http://www.agroseguro.es/Tipos"
		schemaLocation="Tipos.xsd" />

	<xs:element name="Parcela">
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="1" minOccurs="0" name="Ubicacion" type="ti:Ambito"/>
				<xs:element maxOccurs="1" minOccurs="1" name="Cosecha" type="Cosecha"/>
				<xs:element maxOccurs="1" minOccurs="0" name="SIGPAC" type="ti:SIGPAC"/>
			</xs:sequence>
			<xs:attribute name="hoja" type="ti:Parcela_Hoja" use="required"/>
			<xs:attribute name="numero" type="ti:Parcela_Numero" use="required"/>
			<xs:attribute name="nombre" type="ti:Parcela_Nombre" use="optional"/>
			<xs:attribute name="parcelaAgricola" type="ti:Codigo5" use="optional"/>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="Cosecha">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="1" name="CapitalesAsegurados" type="CapitalesAsegurados">
				<xs:key name="CapitalAsegurado_PK">
					<xs:selector xpath="CapitalAsegurado"/>
					<xs:field xpath="@tipo"/>
				</xs:key>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="cultivo" type="ti:Cultivo_Codigo" use="required"/>
		<xs:attribute name="variedad" type="ti:Variedad_Codigo" use="required"/>
	</xs:complexType>

	<xs:complexType name="CapitalesAsegurados">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="1" name="CapitalAsegurado" type="CapitalAsegurado"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CapitalAsegurado">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="0" name="DatosVariables" type="dv:DatosVariables"/>
		</xs:sequence>
		<xs:attribute default="0" name="tipo" type="ti:TipoCapital" use="optional"/>
		<xs:attribute name="superficie" type="ti:Parcela_Superficie" use="required"/>
		<xs:attribute name="precio" type="ti:Precio" use="optional"/>
		<xs:attribute name="produccion" type="ti:ProduccionKG" use="required"/>
	</xs:complexType> 
	
</xs:schema>