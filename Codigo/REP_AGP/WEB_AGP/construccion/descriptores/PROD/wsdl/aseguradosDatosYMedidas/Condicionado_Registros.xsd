	   	<xs:attribute name="var" type="ti:Variedad" use="required" />				    	
	</xs:attributeGroup>
	
	<xs:attributeGroup name="EspecieAgricolaOpcional">
	   	<xs:attribute name="cul" type="ti:Cultivo" use="optional" />
	   	<xs:attribute name="var" type="ti:Variedad" use="optional" />				    	
	</xs:attributeGroup>
		
	<xs:attributeGroup name="AmbitoGeografico">
