	</xs:attributeGroup>
	
	
	<xs:complexType name="Cultivos" ><!-- 1 -->
		<xs:attribute name="cul" type="ti:Cultivo" use="required" />
		<xs:attribute name="desc" type="ti:Descripcion25" use="required" />
	</xs:complexType>

    <xs:complexType name="Variedades"><!-- 2 -->
    	<xs:attribute name="cul" type="ti:Cultivo" use="required" />
    	<xs:attribute name="var" type="ti:Variedad" use="required" />				    	
    	<xs:attribute name="desc" type="ti:Descripcion25" use="required" />
    </xs:complexType>
	
    <xs:complexType name="Modulos"><!-- 3 -->
