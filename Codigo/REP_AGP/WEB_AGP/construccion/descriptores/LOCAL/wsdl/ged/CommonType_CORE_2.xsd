			<enumeration value="E" />
			<enumeration value="T" />
			<length value="1" />
		</restriction>
	</simpleType>
	<simpleType name="t_motivo_soli">
		<annotation>
			<documentation>
				Motivo de la solicitud de tasación: 1-“Concesión Operación” 2-“Revisión
				cumplimiento normativo” 3-“Otros”
			</documentation>
		</annotation>
		<restriction base="string">
			<length value="1" />
			<enumeration value="1" />
			<enumeration value="2" />
			<enumeration value="3" />
		</restriction>
	</simpleType>
	<simpleType name="t_cod_docum">
		<annotation>
			<documentation>
				Código de documento IRIS, de dos dígitos. Sus posibles valores están
				recogidos en la tabla referencial TP_DOC.
			</documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="2" />
		</restriction>
	</simpleType>
	<simpleType name="t_nombre_largo">
		<annotation>
			<documentation> Nombre largo de hasta 200 carácteres. </documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="200" />
		</restriction>
	</simpleType>
	<simpleType name="t_indicadorSNVP">
		<annotation>
			<documentation>
				Posibles valores: S=> SI N=> NO V => VENTAS P => POSICION
			</documentation>
		</annotation>
		<restriction base="string">
			<length value="1" />
			<enumeration value="S" />
			<enumeration value="N" />
			<enumeration value="V" />
			<enumeration value="P" />
		</restriction>
	</simpleType>
	<simpleType name="t_cod_registro">
		<annotation>
			<documentation>
				Tipo de dirección registral del Bien. Posibles valores: 00 - REGISTRO DE
				LA PROPIEDAD 01 - CATASTRO 02 - COLEGIOS PROFESIONALES 03 - REGISTRO
				CONSULAR 04 - REGISTRO COOPERATIVAS 06 - REGISTRO DE TRAFICO 07 -
				REGST.MAQUINARIA AGRICOLA 08 - REGISTRO MERCANTIL 09 - REGISTRO
				ENTIDADES B.E. 99 - OTROS
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="00" />
			<enumeration value="01" />
			<enumeration value="02" />
			<enumeration value="03" />
			<enumeration value="04" />
			<enumeration value="06" />
			<enumeration value="07" />
			<enumeration value="08" />
			<enumeration value="09" />
			<enumeration value="99" />
		</restriction>
	</simpleType>
	<simpleType name="t_libro_reg">
		<annotation>
			<documentation>
				Número de Libro de la Dirección Registral de un Bien.
			</documentation>
		</annotation>
		<restriction base="decimal">
			<totalDigits value="5" />
			<fractionDigits value="0" />
		</restriction>
	</simpleType>
	<simpleType name="t_tomo_reg">
		<annotation>
			<documentation>
				Número de Tomo de la Dirección Registral de un Bien.
			</documentation>
		</annotation>
		<restriction base="decimal">
			<totalDigits value="5" />
			<fractionDigits value="0" />
		</restriction>
	</simpleType>
	<simpleType name="t_folio_reg">
		<annotation>
			<documentation> Folio de la Dirección Registral de un Bien.
			</documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="5" />
		</restriction>
	</simpleType>
	<simpleType name="t_valor">
		<annotation>
			<documentation> Valor actual. </documentation>
		</annotation>
		<restriction base="decimal">
			<totalDigits value="15" />
			<fractionDigits value="2" />
		</restriction>
	</simpleType>
	<simpleType name="t_total_intereses">
		<annotation>
			<documentation> Total intereses antes de amortizar </documentation>
		</annotation>
		<restriction base="decimal">
			<totalDigits value="15" />
			<fractionDigits value="2" />
		</restriction>
	</simpleType>
	<simpleType name="t_porcentaje">
		<annotation>
			<documentation> Porcentaje </documentation>
		</annotation>
		<restriction base="decimal">
			<totalDigits value="6" />
			<fractionDigits value="3" />
		</restriction>
	</simpleType>
	<simpleType name="t_ac_ambito_lci">
		<annotation>
			<documentation> Acuerdo dentro del ámbito de la ley LCI
			</documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="2" />
		</restriction>
	</simpleType>
	<simpleType name="t_capital_pdte">
		<annotation>
			<documentation> Capital pendiente antes de amortizar </documentation>
		</annotation>
		<restriction base="decimal">
			<totalDigits value="15" />
			<fractionDigits value="2" />
		</restriction>
	</simpleType>
	<simpleType name="t_tipo_actualizacion">
		<annotation>
			<documentation> Tipo de actualización </documentation>
		</annotation>
		<restriction base="decimal">
			<totalDigits value="15" />
			<fractionDigits value="2" />
		</restriction>
	</simpleType>
	<simpleType name="t_tipo_doc">
		<annotation>
			<documentation>
				Tipo de documento. Posibles valores: F - Fijo, V - Variable, S -
				Subrogación .
			</documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="1" />
			<enumeration value="F" />
			<enumeration value="V" />
			<enumeration value="S" />
		</restriction>
	</simpleType>
	<simpleType name="t_indicador_validacion">
		<annotation>
			<documentation>
				Indica si se valida sobre movimientos de cuenta o de tarjeta. Valores
				posibles: "C" - Cuenta "D" - Tarjeta de débito
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="C" />
			<enumeration value="D" />
		</restriction>
	</simpleType>
	<simpleType name="t_importe">
		<annotation>
			<documentation> Importe </documentation>
		</annotation>
		<restriction base="decimal">
			<totalDigits value="15" />
			<fractionDigits value="2" />
		</restriction>
	</simpleType>
	<simpleType name="t_formaPago">
		<annotation>
			<documentation>
				Forma de pago actual de la tarjeta: contado - Forma de pago al contado.
				importeFijo - Forma de pago con un importe fijo. porcentaje - Forma
				de pago con un porcentaje aplicado. cuotaFijaMensual - Forma de pago
				con una cuota fija mensual en la tarjeta. porcentajeMensual - Forma
				de pago con un porcentaje mensual en la tarjeta.
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="contado" />
			<enumeration value="importeFijo" />
			<enumeration value="porcentaje" />
			<enumeration value="cuotaFijaMensual" />
			<enumeration value="porcentajeMensual" />
		</restriction>
	</simpleType>
	<simpleType name="t_codigoIdioma">
		<annotation>
			<documentation> Código del idioma. </documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="5" />
		</restriction>
	</simpleType>
	<simpleType name="t_hora_minuto">
		<annotation>
			<documentation> Hora en formato HH:MM </documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="5" />
			<pattern value="([0-1][0-9]|2[0-3]):[0-5][0-9]" />
		</restriction>
	</simpleType>
</schema>