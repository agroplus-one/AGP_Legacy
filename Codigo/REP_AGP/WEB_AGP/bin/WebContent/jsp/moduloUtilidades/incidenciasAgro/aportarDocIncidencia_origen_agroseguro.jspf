<fieldset style="width:auto; text-align:left;">	
	<legend>
		<c:if test="${not consulta}">
			<input type="radio" class="literal" value="am" name="tipoEnvio">
		</c:if>
		<!-- MODIF TAM (14.06.2018). En la consulta que se muestre también el radio button pero bloqueado -->
		<c:if test="${consulta}">
			<input type="radio" class="literal" value="am" name="tipoEnvio" disabled="disabled"/>
