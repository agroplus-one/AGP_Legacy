	<queryString>
		<![CDATA[SELECT VAR.CODCONCEPTO,VAR.CODCONCEPTOPPALMOD,PPAL.DESCONCEPTOPPALMOD,VAR.CODRIESGOCUBIERTO,RI.DESRIESGOCUBIERTO,
       DD.NOMCONCEPTO,VAR.VALOR,VAR.TIPOMODIFICACION,dvp.DESCRIPCION
  FROM o02agpe0.TB_ANEXO_MOD_CAPITALES_DTS_VBL VAR
       inner join o02agpe0.tb_anexo_mod_capitales_aseg cap on cap.id = VAR.IDCAPITALASEGURADO
       inner join o02agpe0.tb_anexo_mod_parcelas par on par.id =cap.idparcelaanexo
       inner join o02agpe0.tb_anexo_mod ane on ane.id = par.idanexo
       inner join o02agpe0.TB_SC_DD_DIC_DATOS DD on (VAR.CODCONCEPTO = DD.CODCONCEPTO)
       left join o02agpe0.TB_SC_C_CONCEPTO_PPAL_MOD PPAL on (PPAL.CODCONCEPTOPPALMOD = VAR.Codconceptoppalmod)
       left outer join o02agpe0.TB_SC_C_RIESGOS_CUBIERTOS RI
            on (RI.codriesgocubierto= VAR.CODRIESGOCUBIERTO and RI.lineaseguroid = $P{LINEASEGUROID} AND RI.codmodulo = ane.codmodulo)
       left join o02agpe0.vw_datos_variables_parcela dvp on (var.codconcepto = dvp.codconcepto and to_char(dvp.codigo) = VAR.Valor)
WHERE VAR.IDCAPITALASEGURADO = $P{ID} and (dvp.lineaseguroid = $P{LINEASEGUROID} or dvp.lineaseguroid = 0 or dvp.lineaseguroid is null)]]>
