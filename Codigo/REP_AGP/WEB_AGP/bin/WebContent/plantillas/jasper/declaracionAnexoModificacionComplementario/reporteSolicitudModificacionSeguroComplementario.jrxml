		<![CDATA[SELECT M.*,
       A.NIFCIF,
       MO.DESMODULO,
       P.CODMODULO AS CODMOD,
       to_char(NVL(c.fecha_envio,sysdate),'DD "de" MONTH "de" YYYY', 'NLS_DATE_LANGUAGE = spanish') as FENVIO
