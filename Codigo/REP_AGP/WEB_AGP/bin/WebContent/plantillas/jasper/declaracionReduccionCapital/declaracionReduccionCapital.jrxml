	<queryString>
		<![CDATA[select distinct
    a.nombre a_nombre, a.apellido1 a_apellido1, a.apellido2 a_apellido2, a.nifcif a_nifcif,
    l.nomlinea l_nombre, l.codplan l_codplan, l.codlinea l_codlinea,
    p.referencia p_referencia, p.dc p_dc,
    c.idcolectivo c_idcolectivo, c.dc c_dc,
    rc.codmotivoriesgo rc_codmotivoriesgo, rc.motivo rc_motivo, rc.fechadanios rc_fechadanios,
    t.ciftomador t_ciftomador, t.razonsocial t_razonsocial,
    to_char(NVL(com.fecha_envio,sysdate),'DD "de" MONTH "de" YYYY', 'NLS_DATE_LANGUAGE = spanish') as FENVIO
from tb_asegurados a, tb_lineas l, tb_tomadores t, tb_polizas p, tb_anexo_red rc left join TB_COMUNICACIONES com on com.idenvio = rc.idenvio, tb_colectivos c
where rc.idpoliza = p.idpoliza
    and p.idasegurado = a.id
    and p.idcolectivo = c.id
    and c.ciftomador = t.ciftomador
    and p.lineaseguroid = l.lineaseguroid
    and a.codentidad = t.codentidad
and rc.id = $P{ID}]]>
