	<queryString>
		<![CDATA[select s.nombre,
       s.id,
       s.apellido1,
       s.apellido2,
       s.razonsocial,
       (s.clavevia || '\ ' || s.direccion) as DIRECCION,
       s.piso,
       s.bloque,
       s.escalera,
       l.nomlocalidad,
       p.nomprovincia,
       s.codpostal,
       s.telefono1,
       s.telefono2,
       s.telefono3,
       s.telefonofijoasegurado,
       s.telefonomovilasegurado,
       s.emailasegurado,
       to_char( s.fechaocurrencia,'DD/MM/YYYY') as FECHAOCURRENCIA,
       s.observaciones,
       s.numsiniestro,
       po.referencia,
       s.idpoliza,
       s.codriesgo || ' - ' || r.desriesgo as riesgo,
       to_char(NVL(c.fecha_envio,sysdate),'DD "de" MONTH "de" YYYY', 'NLS_DATE_LANGUAGE = spanish') as FENVIO
from (((tb_siniestros s join tb_provincias p on s.codprovincia=p.codprovincia)
                        join tb_localidades l on s.codlocalidad=l.codlocalidad)
                        join tb_polizas po on s.idpoliza=po.idpoliza)
						join o02agpe0.tb_lineas li on li.lineaseguroid = po.lineaseguroid
						join o02agpe0.tb_sc_c_lineas lc on lc.codlinea = li.codlinea
                        join o02agpe0.tb_sc_c_riesgos r on r.codriesgo = s.codriesgo and r.codgruposeguro = lc.codgruposeguro
                        left join tb_comunicaciones c on c.idenvio = s.idenvio
where s.id=$P{ID} and s.codprovincia = l.codprovincia and s.sublocalidad = l.sublocalidad]]>
