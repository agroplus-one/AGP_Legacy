	<queryString>
		<![CDATA[SELECT DISTINCT DECODE(FORMAPAGO, 'C', 'COMPLETO', 'FRACCIONADO') AS FORMAPAGO,
                (SELECT ph.FECHA_PAGO
                   FROM o02agpe0.TB_POLIZAS ph
                  WHERE ph.IDPOLIZA = PO.IDPOLIZA) AS FECHAENVIO,
                PP.IMPORTE,
                DECODE(EN.CODENTIDAD, NULL, '', EN.CODENTIDAD || ' - ' || EN.NOMENTIDAD) AS NOMENTIDAD,
                DECODE(PP.DOMICILIADO, 'S', 'SI', 'NO') AS DOMICILIADO,
                NVL(PP.IBAN, '    ') ||
                NVL(PP.CCCBANCO, '                    ') AS IBAN,
                PP.ENVIO_IBAN_AGRO AS ENVIO_IBAN_AGRO,
                DECODE(PP.DEST_DOMICILIACION,
                       'A',
                       DECODE(A.TIPOIDENTIFICACION,
                              'NIF',
                              NVL(A.NOMBRE, '') || ' ' ||
                              NVL(A.APELLIDO1, '') || ' ' ||
                              NVL(A.APELLIDO2, ''),
                              'CIF',
                              NVL(A.RAZONSOCIAL, '')),
                       'T',
                       NVL(T.RAZONSOCIAL, ''),
                       'O',
                       NVL(PP.TITULAR_CUENTA, ''),
                       '') AS TITULAR,
                DA.NUMERO_AVAL AS NUMERO_AVAL,
                DA.IMPORTE_AVAL AS IMPORTE_AVAL,
                (SELECT DBG.DESCRIPCION
                   FROM o02agpe0.TB_SC_C_DATOS_BUZON_GENERAL DBG,
                        o02agpe0.TB_DISTRIBUCION_COSTES_2015 DC
                  WHERE DBG.CODCPTO = 1018
                    AND DBG.VALOR_CPTO = DC.PERIODO_FRACC
                    AND PO.IDPOLIZA = DC.IDPOLIZA) AS PERIODO
  FROM o02agpe0.TB_PAGOS_POLIZA       PP,
       o02agpe0.TB_POLIZAS            PO,
       o02agpe0.TB_ENTIDADES          EN,
       o02agpe0.TB_DATOS_AVAL_POLIZAS DA,
       o02agpe0.TB_ASEGURADOS         A,
       o02agpe0.TB_COLECTIVOS         C,
       o02agpe0.TB_TOMADORES          T
 WHERE SUBSTR(PP.CCCBANCO, 1, 4) = EN.CODENTIDAD(+)
   AND PO.IDPOLIZA = $P{ID}
   AND PP.IDPOLIZA = PO.IDPOLIZA
   AND PP.IDPOLIZA = DA.IDPOLIZA(+)
   AND PO.IDASEGURADO = A.ID
   AND PO.IDCOLECTIVO = C.ID
   AND T.CIFTOMADOR = C.CIFTOMADOR
   AND T.CODENTIDAD = C.CODENTIDAD]]>
	</queryString>
