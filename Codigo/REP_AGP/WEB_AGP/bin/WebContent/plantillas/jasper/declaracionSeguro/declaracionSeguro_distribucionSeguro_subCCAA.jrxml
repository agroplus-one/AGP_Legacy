<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="declaracionSeguro_distribucionSeguro_subCCAA" pageWidth="372" pageHeight="15" columnWidth="372" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<parameter name="ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT CSUB.CODORGANISMO, SUM(CSUB.IMPORTESUBV) as IMPORTE, O.DESORGANISMO
FROM o02agpe0.TB_DIST_COSTE_SUBVS csub, o02agpe0.TB_POLIZAS PO, o02agpe0.TB_DISTRIBUCION_COSTES CO, o02agpe0.TB_SC_C_ORGANISMOS O
WHERE CO.IDPOLIZA = PO.IDPOLIZA
AND CSUB.CODORGANISMO = O.CODORGANISMO
AND CO.ID = CSUB.IDDISTCOSTE
AND PO.IDPOLIZA=$P{ID}
GROUP BY CSUB.CODORGANISMO,O.DESORGANISMO]]>
	</queryString>
