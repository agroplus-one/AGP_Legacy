       PA.CODCOMARCA,
       PA.CODTERMINO,
       PA.CODCULTIVO,
       PA.CODVARIEDAD,
       SUM(C.SUPERFICIE) AS SUPERFICIE,
       SUM(C.INCREMENTOPRODUCCION) AS PRODUCCION
FROM o02agpe0.TB_PARCELAS PA, o02agpe0.TB_CAPITALES_ASEGURADOS C
