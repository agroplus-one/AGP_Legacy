	<queryString>
		<![CDATA[select codigo, descripcion, sum(unidades) as unidades, sum(metroscuadrados) as metroscuadrados,
       sum(metroslineales)as metroslineales,sum(superficie) as superficie, sum(totalprecio)as totalprecio from (
--unidades
select sum(to_number(dv1.valor)) as unidades,
       null as produccion,
       null as metroscuadrados,
       null as metroslineales,
       null as superficie,
       sum (NVL(REL1.PRECIO * to_number(dv1.valor), 0 )) as  totalprecio,
       tc1.codtipocapital as codigo,
       tc1.destipocapital as descripcion
from  o02agpe0.tb_parcelas p1, o02agpe0.tb_capitales_asegurados ca1, o02agpe0.TB_CAP_ASEG_REL_MODULO REL1,
      o02agpe0.tb_datos_var_parcela dv1, o02agpe0.tb_sc_c_tipo_capital tc1
where  p1.idparcela = ca1.idparcela and  ca1.idcapitalasegurado = rel1.idcapitalasegurado and  ca1.idcapitalasegurado = dv1.idcapitalasegurado
       and  dv1.codconcepto = tc1.codconcepto and  ca1.codtipocapital = tc1.codtipocapital AND p1.HOJA = $P{HOJA} and  p1.idpoliza = $P{IDPOLIZA}
       and  ca1.codtipocapital >= 100  and tc1.codconcepto = 117
       group by tc1.codtipocapital, tc1.destipocapital
union --valor fijo
select null as unidades,
       null as produccion,
       null as metroscuadrados,
       null as metroslineales,
       null as superficie,
