      o02agpe0.tb_datos_var_parcela dv1, o02agpe0.tb_sc_c_tipo_capital tc1
where  p1.idparcela = ca1.idparcela and  ca1.idcapitalasegurado = rel1.idcapitalasegurado and  ca1.idcapitalasegurado = dv1.idcapitalasegurado
       and  dv1.codconcepto = tc1.codconcepto and  ca1.codtipocapital = tc1.codtipocapital AND p1.HOJA = $P{HOJA} and  p1.idpoliza = $P{IDPOLIZA}
