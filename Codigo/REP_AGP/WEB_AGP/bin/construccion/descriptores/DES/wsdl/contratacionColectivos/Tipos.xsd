<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
	targetNamespace="http://www.agroseguro.es/Tipos"
	xmlns:ti="http://www.agroseguro.es/Tipos"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
>

	<!-- Definicion de tipos  -->

	<xs:simpleType name="SN">
		<xs:restriction base="xs:token">
			<xs:length value="1" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Alfanumerico_Solo_Numeros">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]*" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Siniestro_NumeroInterno">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="6" />
			<xs:minInclusive value="1" />
		</xs:restriction>	
	</xs:simpleType>

	<xs:simpleType name="Opcion">
		<xs:restriction base="xs:string">
			<xs:length value="1" />
		</xs:restriction>	
	</xs:simpleType>
	
	<xs:simpleType name="Documento_Asunto">
		<xs:restriction base="xs:token">
			<xs:maxLength value="6" />
			<xs:pattern value="[A-Z]*" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="TipoModificacion">
		<xs:restriction base="xs:token">
			<xs:length value="1" />
			<xs:pattern value="[ABM]" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Documento_Asuntos">
		<xs:list itemType="ti:Documento_Asunto" />
	</xs:simpleType>
	
	<xs:simpleType name="Modulo_ConceptoPrincipal">
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="3" />
			<xs:minInclusive value="1" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CodigoRiesgosCubiertos">
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="3" />
			<xs:minInclusive value="1" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Socio_Numero">
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="3" />
			<xs:minInclusive value="1" />
		</xs:restriction>
	</xs:simpleType>
		
	<xs:simpleType name="Serie">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="4" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Acta_Numero">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="7" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Documento_Numero">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="7" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Documento_Tipo">
		<xs:restriction base="ti:Indicador_Estado">
			<xs:enumeration value="A" />
			<xs:enumeration value="S" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Pago_Fase">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="3" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Apellido">
		<xs:restriction base="xs:token">
			<xs:maxLength value="40" />
			<xs:pattern value="\D{1,40}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CCC_Banco">
		<xs:restriction base="ti:Alfanumerico_Solo_Numeros">
			<xs:length value="4" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CCC_Oficina">
		<xs:restriction base="ti:Alfanumerico_Solo_Numeros">
			<xs:length value="4" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CCC_DC">
		<xs:restriction base="ti:Alfanumerico_Solo_Numeros">
			<xs:length value="2" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CCC_Cuenta">
		<xs:restriction base="ti:Alfanumerico_Solo_Numeros">
			<xs:length value="10" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CIF" >
		<xs:restriction base="xs:token">
			<xs:minLength value="3" />
			<xs:maxLength value="9" />
			<xs:pattern value="([A-JN-WYZ]{1}[0-9]{1,7}[A-Z0-9]{1})" />			
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Cobertura_Modulo">
		<xs:restriction base="xs:string">
			<xs:length value="5" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CodigoInternoAgrupacion">
		<xs:restriction base="xs:token">
			<xs:maxLength value="21" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CodigoInternoEntidad">
		<xs:restriction base="xs:token">
			<xs:maxLength value="20" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CodigoPostal">
		<xs:restriction base="xs:token">
			<xs:length value="5" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Colectivo_Referencia">
		<xs:restriction base="xs:token">
			<xs:length value="7" />
			<xs:pattern value="[1]{1}[0-9]{6}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Colectivo_Orden">
		<xs:restriction base="xs:int" >
			<xs:totalDigits value="3" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Comarca_Codigo">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="2" />
			<xs:minInclusive value="0" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Cultivo_Codigo">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="3" />
			<xs:minInclusive value="1" />			
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Indicador_Estado" >
		<xs:restriction base="xs:string">
			<xs:length value="1" />
		</xs:restriction>	
	</xs:simpleType>
	
	<xs:simpleType name="Acta_EstadoAnulacion">
		<xs:restriction base="ti:Indicador_Estado">
			<xs:enumeration value="A" />
			<xs:enumeration value=" " />
		</xs:restriction> 	
	</xs:simpleType>	

	<xs:simpleType name="Acta_EstadoPago">
		<xs:restriction base="ti:Indicador_Estado">
			<xs:enumeration value="P" />
			<xs:enumeration value=" " />
		</xs:restriction> 	
	</xs:simpleType>	
	
	<xs:simpleType name="Fase_Codigo">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="4" />
			<xs:minInclusive value="1" />
			<xs:maxInclusive value="9999" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Motivo_Codigo">
		<xs:restriction base="ti:Riesgo_Codigo" />
	</xs:simpleType>

	<xs:simpleType name="Motivo_Descripcion">
		<xs:restriction base="xs:token">
			<xs:maxLength value="20" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="DigitoControl">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="1" />
			<xs:minInclusive value="0" />
			<xs:maxInclusive value="6" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Direccion_Piso">
		<xs:restriction base="xs:token">
			<xs:maxLength value="5" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Direccion_Bloque">
		<xs:restriction base="xs:token">
			<xs:maxLength value="3" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Direccion_Escalera">
		<xs:restriction base="xs:token">
			<xs:maxLength value="3" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Directorio">
		<xs:restriction base="xs:long">
			<xs:totalDigits value="9" />
			<xs:minInclusive value="0" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Entidad_Codigo">
		<xs:restriction base="xs:token">
			<xs:length value="4" />
			<xs:pattern value="[CM]{1}[0-9]{3}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Email">
		<xs:restriction base="xs:token">
			<xs:maxLength value="50" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="FormaPago">
		<xs:restriction base="xs:token">
			<xs:length value="1" />
			<xs:pattern value="[CF]{1}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="GrupoSeguro">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="3" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="IdentificacionCatastral_Parcela" >
		<xs:restriction base="xs:token">
			<xs:maxLength value="5" />
			<xs:minLength value="1" />
			<xs:pattern value=".*\d+.*" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="IdentificacionCatastral_Poligono">
		<xs:restriction base="xs:token">
			<xs:maxLength value="3" />
			<xs:minLength value="1" />
			<xs:pattern value=".*\d+.*" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ImportePositivo">
		<xs:restriction base="xs:decimal">
			<xs:maxInclusive value="999999999.99" />
			<xs:minInclusive value="0" />
			<xs:fractionDigits value="2" />
			<xs:totalDigits value="11" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="ImporteLargoPositivo">
		<xs:restriction base="xs:decimal">
			<xs:maxInclusive value="99999999999.99" />
			<xs:minInclusive value="0" />
			<xs:fractionDigits value="2" />
			<xs:totalDigits value="13" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ImporteRetribucionMediador">
		<xs:restriction base="ti:ImportePositivo">
			<xs:maxInclusive value="99999.99" />
			<xs:totalDigits value="7" />
			<xs:fractionDigits value="2" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Importe">
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="2" />
			<xs:totalDigits value="11" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ImporteLargo">
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="2" />
			<xs:totalDigits value="13" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ImportePositivo_Liquidacion">
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="4" />
			<xs:totalDigits value="13" />
			<xs:minInclusive value="0" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Linea_Codigo">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="3" />
			<xs:minInclusive value="1" />
			<xs:maxInclusive value="999" />			
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Localidad">
		<xs:restriction base="xs:token">
			<xs:maxLength value="30" />
		</xs:restriction>
	</xs:simpleType>

	 <xs:simpleType name="LimiteSubv">
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="2" />
			<xs:totalDigits value="8" />
			<xs:minInclusive value="0" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="CoeficienteModulacion">
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="4" />
			<xs:totalDigits value="5" />
		</xs:restriction>
	</xs:simpleType>	
	<xs:simpleType name="Mediador_Tipo">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="2" />
			<xs:minInclusive value="1" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Medida_Codigo">
		<xs:restriction base="xs:token">
			<xs:maxLength value="2" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="MedidaReglamento">
		<xs:restriction base="xs:token">
			<xs:length value="1" />
		</xs:restriction>	
	</xs:simpleType>

	<xs:simpleType name="CodigoFechaComputoDias">
		<xs:restriction base="xs:token">
			<xs:length value="1" />
			<xs:pattern value="[VP]" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="MarcaCondicionesParticulares">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="1" />
			<xs:pattern value="[1-4]" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="NIF">
		<xs:restriction base="xs:token">
			<xs:minLength value="2" />
			<xs:maxLength value="9" />
			<xs:pattern value="[LMXY0-9]{1}[0-9]{0,7}[A-Z]{1}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="NIFCIF">
		<xs:union memberTypes="ti:NIF ti:CIF" />
	</xs:simpleType>

	<xs:simpleType name="NombrePersona">
		<xs:restriction base="xs:token">
			<xs:maxLength value="20" />
			<xs:pattern value="\D{1,20}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Parcela_Hoja">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="3" />
			<xs:minInclusive value="1" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Parcela_Hoja_Tradicional">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="3" />
			<xs:minInclusive value="0" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Parcela_Nombre">
		<xs:restriction base="xs:token">
			<xs:maxLength value="20" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Parcela_Numero">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="3" />
			<xs:minInclusive value="1" />			
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Parcela_Superficie">
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="9" />
			<xs:fractionDigits value="2" />
			<xs:minInclusive value="0" />
			<xs:maxInclusive value="9999999.99" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="SuperficieSiniestrada">
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="8" />
			<xs:fractionDigits value="2" />
			<xs:minInclusive value="0" />
			<xs:maxInclusive value="99999999.99" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Provincia_Descripcion">
		<xs:restriction base="xs:token">
			<xs:maxLength value="30" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="NumeroActasPagadas">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="9" />
		</xs:restriction>	
	</xs:simpleType>
	
	<xs:simpleType name="Persona_Tipo" >
		<xs:restriction base="xs:int">
			<xs:totalDigits value="1" />
			<xs:enumeration value="1" />
			<xs:enumeration value="2" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Plan">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="4" />
			<xs:maxInclusive value="9999" />
			<xs:minInclusive value="1980" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Poliza_Referencia_Tipo" >
		<xs:restriction base="xs:token">
			<xs:length value="1" />
			<xs:enumeration value="P" />
			<xs:enumeration value="C" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Poliza_Referencia_Creciente" >
		<xs:restriction base="xs:token">
			<xs:length value="7" />
			<xs:pattern value="[A-HJ-NP-TV-Z]{1}[0-9]{6}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Poliza_Referencia_Tradicional" >
		<xs:restriction base="xs:token">
			<xs:length value="7" />
			<xs:pattern value="[0-9]{6}[A-HJ-PQ-TV-Z]" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Poliza_Referencia" >
		<!-- xs:union memberTypes="ti:Poliza_Referencia_Tradicional ti:Poliza_Referencia_Creciente" / -->
		<xs:restriction base="xs:token">
			<xs:length value="7" />
			<xs:pattern value="(([A-HJ-NP-TV-Z][0-9]{6})|([0-9]{6}[A-HJ-PQ-TV-Z]))" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Poliza_Referencia_Unificada" >
		<xs:annotation>
			<xs:documentation>
				Este tipo se crea para admitir las polizas de ganado renovable que
				tienen una estructura que no coincide ni con tradicional ni con creciente
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:length value="7" />
		</xs:restriction>
	</xs:simpleType>	
	
	<xs:simpleType name="ExclusionSubvenciones_Codigo" >
		<xs:restriction base="xs:int">
			<xs:totalDigits value="2" />
			<xs:minInclusive value="0" />
			<xs:maxInclusive value="99" />
		</xs:restriction>	
	</xs:simpleType>

	<xs:simpleType name="ExclusionSubvenciones_Descripcion" >
		<xs:restriction base="xs:token">
			<xs:maxLength value="40" />
			<xs:minLength value="1" />
		</xs:restriction>	
	</xs:simpleType>

	<xs:simpleType name="Liquidacion_TipoMovimiento">
		<xs:restriction base="xs:int" />
	</xs:simpleType>	
	
	<xs:simpleType name="Organismo_Codigo">
		<xs:restriction base="xs:token">
			<xs:length value="1" />
		</xs:restriction>	
	</xs:simpleType>
	
	<xs:simpleType name="Aplicacion_AnuladaRefundida">
		<xs:restriction base="xs:string">
			<xs:length value="1" />
			<xs:enumeration value=" " />
			<xs:enumeration value="A" />
			<xs:enumeration value="R" />			
		</xs:restriction>	
	</xs:simpleType>

	<xs:simpleType name="BonificacionRecargo_Tipo">
		<xs:restriction base="xs:string">
			<xs:length value="1" />
			<xs:enumeration value=" " />			
			<xs:enumeration value="C" />
			<xs:enumeration value="R" />			
		</xs:restriction>	
	</xs:simpleType>
	
	<xs:simpleType name="TantoPorCien">
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="5" />
			<xs:fractionDigits value="2" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="MedidasAsegurado_Tipo">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="1" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Precio">
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0" />
			<xs:maxInclusive value="99999.9999" />
			<xs:fractionDigits value="4" />
			<xs:totalDigits value="9" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Coeficiente_Rendimiento">
		 
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0.01" />
			<xs:maxInclusive value="1.99" />
			<xs:fractionDigits value="2" />
			<xs:totalDigits value="3" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Acta_CodigoIndemnizable">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="1" />
			<xs:enumeration value="0" />
			<xs:enumeration value="1" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Produccion">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="4" />
			<xs:minInclusive value="0" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ProduccionKG">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="9" />
			<xs:minInclusive value="0" />			
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Provincia_Codigo">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="2" />
			<xs:minInclusive value="1" />
			<xs:maxInclusive value="50" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Provincia_Codigo_Generico">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="2" />
			<xs:enumeration value="99" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Provincia_Codigo_Con_Generico">
		<xs:union memberTypes="ti:Provincia_Codigo ti:Provincia_Codigo_Generico" />
	</xs:simpleType>

	<xs:simpleType name="RazonSocial_Denominacion">
		<xs:restriction base="xs:token">
			<xs:minLength value="1" />
			<xs:maxLength value="100" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Recibo_Codigo">
		<xs:restriction base="xs:long">
			<xs:totalDigits value="7" />
			<xs:minInclusive value="1" />
			<xs:maxInclusive value="9999999" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Recibo_Tipo">
