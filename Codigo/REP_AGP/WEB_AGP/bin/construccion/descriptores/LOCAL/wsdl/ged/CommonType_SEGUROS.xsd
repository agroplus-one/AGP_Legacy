<schema xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:seg="http://www.ruralserviciosinformaticos.com/XSD/SEGUROS/CommonType_SEGUROS/"
	xmlns:core="http://www.ruralserviciosinformaticos.com/XSD/CORE/CommonType_CORE/"
	xmlns:core2="http://www.ruralserviciosinformaticos.com/XSD/CORE/CommonType_CORE_2/"
	xmlns:xop="http://www.w3.org/2004/08/xop/include"
	targetNamespace="http://www.ruralserviciosinformaticos.com/XSD/SEGUROS/CommonType_SEGUROS/"
	elementFormDefault="qualified">
	<import
		namespace="http://www.ruralserviciosinformaticos.com/XSD/CORE/CommonType_CORE/"
		schemaLocation="http://soa03.risa:80/SOA_Seguros/Empresa/PS/soap/v1/SE_SEG_Documentacion_GED?SCHEMA%2FSOA_ComunV1%2FRecursos%2FXSD%2FCORE%2FCommonType_CORE" />
	<import
		namespace="http://www.ruralserviciosinformaticos.com/XSD/CORE/CommonType_CORE_2/"
		schemaLocation="./CommonType_CORE_2.xsd" />
	<import namespace="http://www.w3.org/2004/08/xop/include"
		schemaLocation="http://soa03.risa:80/SOA_Seguros/Empresa/PS/soap/v1/SE_SEG_Documentacion_GED?SCHEMA%2FSOA_Seguros%2FRecursos%2Fcommon%2FXSD%2FMtom" />
	<!-- P19388 Servicio Web Documentación de Pratus-CAT 24/09/2013 LFS -->
	<simpleType name="t_codcia">
		<annotation>
			<documentation>Compañía de la póliza </documentation>
		</annotation>
		<restriction base="string">
			<minLength value="1" />
			<maxLength value="2" />
		</restriction>
	</simpleType>
	<simpleType name="t_codtec">
		<annotation>
			<documentation>Producto técnico de la póliza </documentation>
		</annotation>
		<restriction base="string">
			<minLength value="1" />
			<maxLength value="4" />
		</restriction>
	</simpleType>
	<simpleType name="t_annumsin">
		<annotation>
			<documentation>Año del siniestro </documentation>
		</annotation>
		<restriction base="int">
			<minInclusive value="1900" />
			<maxExclusive value="10000" />
		</restriction>
	</simpleType>
	<simpleType name="t_numstro">
		<annotation>
			<documentation>Número del siniestro </documentation>
		</annotation>
		<restriction base="string">
			<minLength value="1" />
			<maxLength value="8" />
			<pattern value="\d{1,8}" />
		</restriction>
	</simpleType>
	<simpleType name="t_codmedia">
		<annotation>
			<documentation>Código de mediador de la póliza </documentation>
		</annotation>
		<restriction base="string">
			<minLength value="1" />
			<maxLength value="4" />
		</restriction>
	</simpleType>
	<simpleType name="t_refbdweb">
		<annotation>
			<documentation>
				Referencia del documento en gestor de contenidos de RGA
			</documentation>
		</annotation>
		<restriction base="string">
			<minLength value="1" />
			<maxLength value="20" />
		</restriction>
	</simpleType>
	<simpleType name="t_tipodweb">
		<annotation>
			<documentation>Tipo de documento </documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="4" />
		</restriction>
	</simpleType>
	<simpleType name="t_subtdweb">
		<annotation>
			<documentation>Subtipo de documento </documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="4" />
		</restriction>
	</simpleType>
	<simpleType name="t_codusua">
		<annotation>
			<documentation>Código de usuario </documentation>
		</annotation>
		<restriction base="string">
			<minLength value="1" />
			<maxLength value="20" />
		</restriction>
	</simpleType>
	<simpleType name="t_destinatario">
		<annotation>
			<documentation>Destinatario del documento en el expediente
			</documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="100" />
		</restriction>
	</simpleType>
	<simpleType name="t_nombreDoc">
		<annotation>
			<documentation>Nombre del fichero en servidor FTP </documentation>
		</annotation>
		<restriction base="string">
			<minLength value="1" />
			<maxLength value="30" />
		</restriction>
	</simpleType>
	<simpleType name="t_ref_sesion_rga">
		<annotation>
			<documentation>Referencia única de la petición al Servicio Web
			</documentation>
		</annotation>
		<restriction base="string">
			<minLength value="1" />
			<maxLength value="50" />
		</restriction>
	</simpleType>
	<simpleType name="t_idDoc">
		<annotation>
			<documentation>Identificador único de Documentum </documentation>
		</annotation>
		<restriction base="string">
			<minLength value="1" />
			<maxLength value="16" />
		</restriction>
	</simpleType>
	<simpleType name="t_formatoDoc">
		<annotation>
			<documentation>Extensión del fichero (sin punto) </documentation>
		</annotation>
		<restriction base="string">
			<pattern value="[a-zA-Z0-9]*" />
			<minLength value="1" />
			<maxLength value="10" />
		</restriction>
	</simpleType>
	<simpleType name="t_codigoRetorno">
		<restriction base="short" />
	</simpleType>
	<!-- P19388 Servicio Web de Consultas (Multicanalidad) 30/10/2015 VPS -->
	<complexType name="t_modeloEntrada">
		<annotation>
			<documentation>Objeto de entrada al servicio web ConsultasWS
			</documentation>
		</annotation>
		<sequence>
			<element name="company" type="seg:t_company" />
			<element name="criteria" type="seg:t_Criteria" minOccurs="0"
				maxOccurs="unbounded" />
			<element name="culture" type="seg:t_culture" minOccurs="0" />
			<element name="dataKey" type="seg:t_dataKey" minOccurs="0" />
			<element name="isList" type="seg:t_islist" minOccurs="0" />
			<element name="page" type="seg:t_page" minOccurs="0" />
			<element name="pageSize" type="seg:t_pageSize" minOccurs="0" />
			<element name="resultType" type="seg:t_ResultType" minOccurs="0" />
			<element name="sortProperties" type="seg:t_sortproperties"
				minOccurs="0" />
			<element name="sortOrders" type="seg:t_sortOrders" minOccurs="0" />
			<element name="userName" type="seg:t_userName" minOccurs="0" />
			<element name="workspaceAttribute" type="seg:t_workspaceAttribute"
				minOccurs="0" />
			<element name="workspaceId" type="seg:t_workspaceId"
				minOccurs="0" />
		</sequence>
	</complexType>
	<simpleType name="t_company">
		<annotation>
			<documentation>Compañía que ejecuta la acción </documentation>
		</annotation>
		<restriction base="string">
			<minLength value="1" />
			<maxLength value="15" />
		</restriction>
	</simpleType>
	<complexType name="t_Criteria">
		<annotation>
			<documentation>
				Elemento clave valor con datos para ejecución de transacción
			</documentation>
		</annotation>
		<sequence>
			<element name="name" type="seg:t_name" minOccurs="0" />
			<element name="value" type="seg:t_value" minOccurs="0" />
		</sequence>
	</complexType>
	<simpleType name="t_name">
		<annotation>
			<documentation>Nombre de la clave </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_value">
		<annotation>
			<documentation>Valor de la clave </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_culture">
		<annotation>
			<documentation>Cultura para el formato de los datos </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_dataKey">
		<annotation>
			<documentation>Transacción a la que se realiza la llamada
			</documentation>
		</annotation>
		<restriction base="string">
			<minLength value="1" />
			<maxLength value="10" />
		</restriction>
	</simpleType>
	<simpleType name="t_islist">
		<annotation>
			<documentation>Cultura para el formato de los datos </documentation>
		</annotation>
		<restriction base="boolean" />
	</simpleType>
	<simpleType name="t_page">
		<annotation>
			<documentation>Número de página de petición </documentation>
		</annotation>
		<restriction base="int" />
	</simpleType>
	<simpleType name="t_pageSize">
		<annotation>
			<documentation>Tamaño de la página </documentation>
		</annotation>
		<restriction base="int" />
	</simpleType>
	<simpleType name="t_ResultType">
		<annotation>
			<documentation>Tipo de formato requerido (XML, Json o ByteArray)
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="Json" />
			<enumeration value="Xml" />
			<enumeration value="ByteArray" />
		</restriction>
	</simpleType>
	<simpleType name="t_sortproperties">
		<annotation>
			<documentation>Propiedad sobre la que se ordena </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_sortOrders">
		<annotation>
			<documentation>Tipo de ordenación </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_userName">
		<annotation>
			<documentation>Usuario </documentation>
		</annotation>
		<restriction base="string">
			<minLength value="1" />
			<maxLength value="8" />
		</restriction>
	</simpleType>
	<simpleType name="t_workspaceAttribute">
		<annotation>
			<documentation>Espacio de trabajo </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_workspaceId">
		<annotation>
			<documentation>ID del espacio de trabajo </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<complexType name="t_modeloSalida">
		<annotation>
			<documentation>Objeto de salida al servicio web de Consultas WS
			</documentation>
		</annotation>
		<sequence>
			<element name="codigoError" type="seg:t_codigoError" />
			<element name="descError" type="seg:t_descError" />
			<element name="resultData" type="seg:t_resultData" />
		</sequence>
	</complexType>
	<simpleType name="t_codigoError">
		<annotation>
			<documentation>Código de error </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_descError">
		<annotation>
			<documentation>Descripción de error </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_resultData">
		<annotation>
			<documentation>
				Datos de respuesta del servicio (traducción de la llamada a Json o XML)
			</documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<!-- P19388 Servicio Web de Contratación (Multicanalidad). 30/10/2015 VPS -->
	<complexType name="t_modeloEntradaC">
		<annotation>
			<documentation>Objeto de entrada al servicio web ContratacionWS
			</documentation>
		</annotation>
		<sequence>
			<element name="operationHeader" type="seg:t_operationHeader"
				minOccurs="1" />
			<element name="operationInfo" type="seg:t_operationInfo"
				minOccurs="1" />
		</sequence>
	</complexType>
	<complexType name="t_modeloSalidaC">
		<annotation>
			<documentation>Objeto de salida al servicio web ContratacionWS
			</documentation>
		</annotation>
		<sequence>
			<element name="codigoError" type="seg:t_codigoError" />
			<element name="descError" type="seg:t_descError" />
			<element name="operationHeader" type="seg:t_operationHeader" />
			<element name="operationInfo" type="seg:t_operationInfo" />
			<element name="responseStatus" type="seg:t_responseStatus" />
		</sequence>
	</complexType>
	<complexType name="t_operationHeader">
		<annotation>
			<documentation>Cabecera de la operación </documentation>
		</annotation>
		<sequence>
			<element name="requestInfo" type="seg:t_requestInfo"
				minOccurs="1" />
			<element name="userInfo" type="seg:t_userInfo" minOccurs="1" />
		</sequence>
	</complexType>
	<complexType name="t_requestInfo">
		<annotation>
			<documentation>Información de la llamada </documentation>
		</annotation>
		<sequence>
			<element name="operationCode" type="seg:t_operationCode"
				minOccurs="1" />
			<element name="operationProduct" type="seg:t_operationProduct"
				minOccurs="0" />
			<element name="operationType" type="seg:t_operationType"
				minOccurs="1" />
			<element name="operationDescription" type="seg:t_operationDescription"
				minOccurs="0" />
			<element name="operationTimestamp" type="seg:t_operationTimestamp"
				minOccurs="0" />
			<element name="operationGuid" type="seg:t_operationGuid"
				minOccurs="0" />
			<element name="operationSession" type="seg:t_operationSession"
				minOccurs="0" />
		</sequence>
	</complexType>
	<simpleType name="t_operationCode">
		<annotation>
			<documentation>Código de la operación </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_operationProduct">
		<annotation>
			<documentation>Producto de la operación </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_operationType">
		<annotation>
			<documentation>Tipo o formato de la operación </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_operationDescription">
		<annotation>
			<documentation>Descripción de la operación </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_operationTimestamp">
		<annotation>
			<documentation>Hora de la operación </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_operationGuid">
		<annotation>
			<documentation>Guid de la operación </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_operationSession">
		<annotation>
			<documentation>Sesión de la operación </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<complexType name="t_userInfo">
		<annotation>
			<documentation>Información del usuario </documentation>
		</annotation>
		<sequence>
			<element name="Origin" type="seg:t_origin" minOccurs="1" />
			<element name="Company" type="seg:t_company" minOccurs="1" />
			<element name="Office" type="seg:t_office" minOccurs="0" />
			<element name="Employee" type="seg:t_employee" minOccurs="0" />
			<element name="Terminal" type="seg:t_terminal" minOccurs="0" />
			<element name="Email" type="seg:t_email" minOccurs="0" />
			<element name="Profile" type="seg:t_profile" minOccurs="0" />
			<element name="User" type="seg:t_userName" minOccurs="1" />
			<element name="Culture" type="seg:t_culture" minOccurs="0" />
		</sequence>
	</complexType>
	<simpleType name="t_origin">
		<annotation>
			<documentation>Origen de la operación </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_office">
		<annotation>
			<documentation>Oficina que ejecuta la acción </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_employee">
		<annotation>
			<documentation>Empleado que ejecuta la operación </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_terminal">
		<annotation>
			<documentation>Terminal desde donde se ejecuta la operación
			</documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_email">
		<annotation>
			<documentation>Email responsable de la ejecución de la operación
			</documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="100" />
		</restriction>
	</simpleType>
	<simpleType name="t_profile">
		<annotation>
			<documentation>Perfil responsable de la ejecución de la operación
			</documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<complexType name="t_operationInfo">
		<annotation>
			<documentation>Información de la operación, datos de entrada
			</documentation>
		</annotation>
		<sequence>
			<element name="documentos" type="seg:t_documentos" minOccurs="0" />
			<element name="rawData" type="seg:t_rawData" minOccurs="1" />
		</sequence>
	</complexType>
	<complexType name="t_documentos">
		<annotation>
			<documentation>Array de documentos </documentation>
		</annotation>
		<sequence>
			<element name="documento" type="seg:t_documento" minOccurs="0"
				maxOccurs="unbounded" />
		</sequence>
	</complexType>
	<complexType name="t_documento">
		<annotation>
			<documentation>Representación de un documento </documentation>
		</annotation>
		<sequence>
			<element name="Content" type="seg:t_Content" minOccurs="0" />
			<element name="ContentType" type="seg:t_ContentType"
				minOccurs="0" />
			<element name="DocumentContextType" type="seg:t_DocumentContextType"
				minOccurs="0" />
			<element name="Name" type="seg:t_Name" minOccurs="0" />
			<element name="Publicable" type="seg:t_Publicable" minOccurs="0" />
			<element name="ConBarras" type="seg:t_ConBarras" minOccurs="0" />
		</sequence>
	</complexType>
	<simpleType name="t_Content">
		<annotation>
			<documentation>Array de bytes del documento </documentation>
		</annotation>
		<restriction base="base64Binary" />
	</simpleType>
	<simpleType name="t_ContentType">
		<annotation>
			<documentation>ContentType del documento </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_DocumentContextType">
		<annotation>
			<documentation>Context Type del documento </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_Name">
		<annotation>
			<documentation>Nombre del documento </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_Publicable">
		<annotation>
			<documentation>Documento publicable </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<simpleType name="t_ConBarras">
		<annotation>
			<documentation>Código de barras </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<complexType name="t_rawData">
		<annotation>
			<documentation>Array de Datos (Data) </documentation>
		</annotation>
		<sequence>
			<element name="data" type="seg:t_data" minOccurs="0"
				maxOccurs="unbounded" />
		</sequence>
	</complexType>
	<complexType name="t_data">
		<annotation>
			<documentation>
				Elemento clave valor con datos para ejecución de transacción
			</documentation>
		</annotation>
		<sequence>
			<element name="Key" type="seg:t_name" />
			<element name="Value" type="seg:t_value" />
		</sequence>
	</complexType>
	<complexType name="t_responseStatus">
		<annotation>
			<documentation>
				Array de códigos de respuesta de la ejecución de la operación.
			</documentation>
		</annotation>
		<sequence>
			<element name="ResponseEstatusData" type="seg:t_ResponseEstatusData"
				minOccurs="0" maxOccurs="unbounded" />
		</sequence>
	</complexType>
	<complexType name="t_ResponseEstatusData">
		<annotation>
			<documentation>Elemento de respuesta de la operación </documentation>
		</annotation>
		<sequence>
			<element name="responseCode" type="seg:t_codigoError" />
			<element name="ResponseDescription" type="seg:t_descError" />
			<element name="status" type="seg:t_status" minOccurs="0" />
		</sequence>
	</complexType>
	<simpleType name="t_status">
		<annotation>
			<documentation>Estado o Tipo del error </documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	<!-- P19125 Añadimos los tipos para el WS de Decesos. 10/03/2016 VPS -->
	<simpleType name="t_clase_siniestro">
		<annotation>
			<documentation>
				Clase del siniestro 4 - Rechazo sin intervención 5 - Normal
			</documentation>
		</annotation>
		<restriction base="string">
			<minLength value="1" />
			<maxLength value="1" />
		</restriction>
	</simpleType>
	<simpleType name="t_indicador_gestora">
		<annotation>
			<documentation> Indicador de la gestora que tramita el siniestro
			</documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="1" />
		</restriction>
	</simpleType>
	<simpleType name="t_cod_oper_asistencia">
		<annotation>
			<documentation>Código de la operadora de asistencia </documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="4" />
		</restriction>
	</simpleType>
	<simpleType name="t_cod_causa_asistencia">
		<annotation>
			<documentation>
				Código de la causa de asistencia 51 - Fallecimiento 55 - Fallecimiento
				Neonato 56 - Amputación para enterramiento
			</documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="4" />
		</restriction>
	</simpleType>
	<simpleType name="t_cod_motivo_rechazo">
		<annotation>
			<documentation>
				Código del motivo del rechazo 05-Riesgo no cubierto
			</documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="2" />
		</restriction>
	</simpleType>
	<simpleType name="t_tipo_accion">
		<annotation>
			<documentation>
				Tipo de acción a realizar en el siniestro 1 - Añadir observaciones al
				siniestro 2 - Cambio de tomador
			</documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="2" />
		</restriction>
	</simpleType>
	<simpleType name="t_texto_obs">
		<annotation>
			<documentation>Texto de las observaciones al siniestro
			</documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="1200" />
		</restriction>
	</simpleType>
	<simpleType name="t_origen_obs">
		<annotation>
			<documentation>Origen de las observaciones al siniestro
			</documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="1" />
		</restriction>
	</simpleType>
	<simpleType name="t_num_poliza">
		<annotation>
			<documentation>Número de póliza </documentation>
		</annotation>
		<restriction base="string">
			<minLength value="1" />
